        "@tsconfig/svelte": "^3.0.0",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "eslint": "^8.19.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "obsidian": "latest",
        "svelte": "^3.50.1",
        "svelte-check": "^2.8.0",
        "svelte-preprocess": "^4.10.7",
        "tslib": "2.4.0",
        "typescript": "4.8.3",
        "vite": "^3.1.3"
